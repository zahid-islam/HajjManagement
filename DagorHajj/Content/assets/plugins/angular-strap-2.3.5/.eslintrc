{
  "extends": "airbnb/legacy",
  "rules": {
    "strict": [2, "global"],
    "comma-dangle": [2, "never"],
    "func-names": 0,
    "padded-blocks": 0,
    "no-use-before-define": [2, "nofunc"],
    "no-unused-vars": 1,
    "vars-on-top": 1,
    "id-length": [2, {"properties": "never", "exceptions": ["i", "j", "k", "l", "x", "y", "_"]}]
  },
  "globals": {
    "angular": 1
  }
}
